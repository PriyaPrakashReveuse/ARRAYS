#include <iostream>

using namespace std;

int LinearSearch (int arr[], int n, int key)
{
    for(int i=0;i<n;i++)
    {
        if(arr[i]==key)
        {
          return i;
        }
    }
    return -1;
}

int main ()
{
  int m;
  cin >> m;
  int arr[m];
  
  for (int i = 0; i < m; i++)
  {
      cin >> arr[i];
   }
    
  int key = 0;
  cin >> key;
  
  cout<<LinearSearch(arr,m,key);
  return 0;
  
}
