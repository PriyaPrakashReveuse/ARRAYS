#include <iostream>
#include<climits>
using namespace std;

int main()
{
    int m;
    cin>>m;
    int arr[m];
    for(int i=0;i<m;i++)
    {
        cin>>arr[i];
    }
    
    int maxNo=INT_MIN,minNo=INT_MAX;
    for(int i=0;i<m;i++)
    {
        maxNo=max(maxNo,arr[i]);
        minNo=min(minNo,arr[i]);
    }
    cout<<maxNo<<" "<<minNo<<endl;
    return 0; 
}
